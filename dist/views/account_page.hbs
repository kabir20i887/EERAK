                    let currentInp = inp
                    var key = e.which || e.keyCode
                    var ctrl = e.ctrlKey ? e.ctrlKey : ((key === 17) ? true : false);
                    if (key == 86 && ctrl) {
                        pasteEv = true
                    }
                    if (currentInp.value.length > 1) {

