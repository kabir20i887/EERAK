                        cu11.style.background = 'rgb(246 245 245)'
                        cu11.style.borderBottom = '2px solid rgb(198 198 198)'

                    })
                    if (!pasteEv) {
                        nextInp = inp.nextElementSibling
                        previousInp = inp.previousElementSibling

                        console.log(nextInp)
                        //
                        if (nextInp && nextInp.hasAttribute('disabled') && currentInp.value !== '') {
                            console.log(nextInp)
                            nextInp.removeAttribute('disabled')
                            nextInp.focus()
                        }
                        console.log(e.key)
                        if (e.key === 'Backspace') {
                            inputBx.forEach((inp11, index2) => {
                                if (index2 < index) {
                                    currentInp.setAttribute('disabled', true)
                                    currentInp.value = ''
                                    previousInp.removeAttribute('disabled')
                                    previousInp.focus()
                                }

                            })
                        }

                    }

                })
            });
