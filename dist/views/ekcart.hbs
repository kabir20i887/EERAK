                            let data = cu.data()
                            let id = data.rootAutyh
                            let fstPur = false
                            let locFree = false
                            //checking from the locastorage data
                            if (elem.permGroup !== undefined) {
                                elem.permGroup.forEach(cur => {
                                    if (cur.price === 'Free') {
                                        locFree = true
                                    }
                                })
                            }


                            //if there is free perm data 
                            if (locFree) {
                                if (id === JSON.parse(getCookie('user')).rootAutyh) {
                                    fstPur = data.fstPurchase
                                    if (!fstPur && locFree) {
                                        cheated = true
                                    }
                                    let objF = JSON.parse(getCookie('user'))
                                    objF.fstPurchase = fstPur
                                    document.cookie = "user" + "=" + JSON.stringify(objF) + ";" + "expires=18 Dec 9999 12:00:00 UTC;path=/"


                                }
                            }
                        })

                        // end................

                        if (allfd.length !== 0) {
                            let price = parseFloat(elem.price.split('₹')[1].split(',').join(''))

                            addToCart(elem, '.bx_evt_cart_item ul', price, i, cheated)

                        } else {
                            if (allfd.length !== 0) {
                                document.querySelector('.ek_cart').style.height = '100vh'

                                document.querySelector('.for_ek_fd_cart_load').style.display = 'none'
                                document.querySelector('.for_ek_food_cart').style.display = 'flex'
                                document.querySelector('.ct_subotatal').style.display = 'none'
                                document.querySelector('.ct_list_food').style.borderBottom = 'none'
                                document.querySelector('.no_req .ct_list_food').style.width = '97%'
                                document.querySelector('.ct_bin_ts_nav_ck ').style.display = 'none'
                            }

                        }
                    })
                }
                goForCart()


            }
        }





















    </script>

