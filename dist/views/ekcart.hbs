            }

            50% {
                background: rgb(165, 166, 167);
            }

            100% {
                transform: translate(-50%, -50%) rotate(360deg);
                background: rgb(143, 144, 146);
            }
        }

        @keyframes horizontalProgressBar {
            0% {
                width: 0%;
            }

            20% {
                width: 10%;
            }

            30% {
                width: 15%;
            }

            40% {
                width: 18%;
            }

            50% {
                width: 20%;
            }

            60% {
                width: 22%;
            }

            100% {
                width: 85%;
            }
        }

