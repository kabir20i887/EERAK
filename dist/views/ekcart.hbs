    <style>
        .spinner-square {
            display: flex;
            flex-direction: row;
            width: 90px;
            height: 120px;
            position: absolute;
            top: 50%;
            left: 50%;
            display: none;
            transform: translate(-50%, -50%);
            z-index: 9999999999999;
        }

        a {
            color: #007bff;
            text-decoration: none;
            background-color: transparent;
            -webkit-text-decoration-skip: objects;
        }

        .ct_outer_flex {
            display: flex;
            justify-content: center;
        }

        .crousel_items .ct_flex_offer {
            background-color: rgb(255, 0, 64);
            padding: 3px 15px;
            margin: 10px 0 0 4px;
            width: max-content;

        }

        .ct_flex_offer p {
            color: white;
            font-size: 12px;
            font-family: 'roboto';
        }

        .ct_popup_main_sucess a {
            font-size: 14px;
            display: block;
            color: black;
            margin: 0px 21px;
            width: max-content;
            text-decoration: revert;
        }

        .ct_popup_main_sucess a ion-icon {
            transform: translate(4px, 3px);
        }

        .spinner-square>.square {
            width: 17px;
            height: 80px;
            margin: auto auto;
            border-radius: 4px;
        }

        .ct_popup_main_sucess .remove_btn {
            position: absolute;
            color: black;
            font-size: 30px;
            position: absolute;
            right: 0;
            top: -41px;
        }

        .ct_show_all_cpn {
            padding: 2px 14px;
            text-align: center;
            font-size: 14px;
            font-family: 'roboto';
            color: black;
            text-decoration: underline;
        }

        .square-1 {
            animation: square-anim 1200ms cubic-bezier(0.445, 0.05, 0.55, 0.95) 0s infinite;
        }

        .square-2 {
            animation: square-anim 1200ms cubic-bezier(0.445, 0.05, 0.55, 0.95) 200ms infinite;
        }

        .square-3 {
            animation: square-anim 1200ms cubic-bezier(0.445, 0.05, 0.55, 0.95) 400ms infinite;
        }

        @keyframes square-anim {
            0% {
                height: 80px;
                background-color: rgb(240, 216, 111);
            }

            20% {
                height: 80px;
            }

            40% {
                height: 120px;
                background-color: rgb(238, 212, 96);
            }

            80% {
                height: 80px;
            }

            100% {
                height: 80px;
                background-color: rgb(241, 212, 82);
            }
        }

        .ct_load_review_verf .ct_load_items {
            padding: 0;
            background: transparent;
            display: flex;
            justify-content: center;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            box-shadow: none
        }

        .ct_load_review_verf {
            background-color: #ffffff96;
            height: 100vh;
            width: 100vw;
            z-index: 9999999999;
            position: fixed;
            display: none;
        }

