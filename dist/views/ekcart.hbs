            let bx = e.target.closest('.ct_bx_qty_inner')
            let bxA = e.target.closest('.ct_bx_units_item li')
            let addReq = e.target.closest('.Add_cart')
            let remMsg = e.target.closest('.ct_remove_bill_pop_up_update_acc')
            let removeBsitm = e.target.closest('.remove_base_itm')
            let addBsitm = e.target.closest('.add_base_itm')
            let info_cpns = e.target.closest('.ct_info_btn')
            let check_code_cpns = e.target.closest('.ct_search_cpns button')
            let popup_succ = e.target.closest('.ct_popup_main_sucess')
            let rem_pop_succ = e.target.closest('.remove_btn')
            let bacck_btn = e.target.closest('.ct_back_btn');
            let forward_btn = e.target.closest('.ct_forward_btn');

            let product = document.getElementsByClassName('product')
            let product_page = Math.ceil(product.length) - 1;
            let maxMove = 99203;
            let mob_view = window.matchMedia("(max-width: 768px)");

            if (mob_view.matches) {
                movePer = 50.36;
                maxMove = 504;
            }
         
   

            let addItem = e.target.closest('.ct_addbtns');

            if (!popup_succ || rem_pop_succ) {
                document.querySelector('.ct_popup_main_sucess').style.display = 'none'
                document.querySelector('.ct_bx_ov_lay_x').style.display = 'none'
                document.querySelector('.confetti').style.display = 'none'

            }

            if (addItem !== null) {
                let typ_fd = addItem.classList.value.split(' ')[1]
                console.log(addItem.parentElement.childNodes)
                addItem.classList.add('added')
                addItem.style.pointerEvents = 'none'
                addItem.childNodes[1].innerText = 'Added to cart'
                tot_pr_itm = 0
                tot_pr_itm2 = 0
                if (window.screen.width <= 1100) {
                    document.querySelector('.place_order_btn_bottom').style.display = 'block'
                }
                if (typ_fd === 'sauce') {
                    let allfd = JSON.parse(localStorage.getItem('sauce'))
                    console.log(addItem.parentElement.childNodes[1].childNodes[3].childNodes)

                    let inner_bx = addItem.parentElement.childNodes[1].childNodes[3].childNodes
                    // price , nt weight, qty
                    let obj = {
                        name: inner_bx[5].innerText,
                        qty: 1,
                        slt_nm: inner_bx[9].innerText.split(':')[1].split(' ').join(''),
                        slt_p: "pouch pack",
                        type: "sauce",

                    }
                    allfd.push(obj)
                    localStorage.setItem('sauce', JSON.stringify(allfd))

                    addItems('sauce', false)

                } else if (typ_fd === 'essen') {
                    let allfd = JSON.parse(localStorage.getItem('essentials'))

                    let nm = addItem.parentElement.childNodes[1].innerText
                    let unit = addItem.parentElement.childNodes[3].childNodes[1].childNodes[1].childNodes[1].innerText
                    let obj = { name: nm, qt: 1, unit: unit }
                    allfd.push(obj)
                    localStorage.setItem('essentials', JSON.stringify(allfd))
                    addItems('essen', false)

                }
            }
            if (forward_btn !== null) {
                let product_width = document.getElementsByClassName('product')[0].getBoundingClientRect().width + 8
                let movePer = product_width;
                count_bx++

                if (product_page >= count_bx) {
                    l = l + movePer;



                    if (product == 1) { l = 0; }
                    for (const i of product) {


                        // l = l - movePer; 
                        i.style.left = '-' + l + 'px';

                    }
                } else {
                    count_bx = product_page
                }


            }
            if (bacck_btn !== null) {
                let product_width = document.getElementsByClassName('product')[0].getBoundingClientRect().width + 8
                let movePer = product_width;
                l = l - movePer;
                if (l <= 0) { l = 0; }
                if (count_bx > 0) {

                    count_bx--
                    for (const i of product) {
                        i.style.left = '-' + l + 'px';

                    }
                }

            }


            if (info_cpns !== null) {
                if (info_cpns.parentElement.parentElement.childNodes[3].classList.value.includes('active')) {
                    info_cpns.parentElement.parentElement.childNodes[3].classList.remove('active')

                } else {
                    info_cpns.parentElement.parentElement.childNodes[3].classList.add('active')

                }
            } else {
                if (document.querySelector('.ct_info_cpns.active') !== null) {
                    document.querySelector('.ct_info_cpns.active').classList.remove('active')
                }
            }
            if (check_code_cpns !== null) {
                document.querySelector('.ct_coupons_all .ct_inner_itms').innerHTML = ''

                document.querySelector('.ct_coupons_all .ct_load_items').style.display = 'flex'
                let code = check_code_cpns.parentElement.childNodes[1].value
                async function cpns() {
                    let dbCollection = db.collection("coupons")
                    let res = await dbCollection.orderBy('grp').get()
                    let found = false
                    res.docs.forEach(async cur => {
                        let dt = cur.data()
                        if (dt.secret !== null) {
                            console.log(code)
                            if (dt.secret === code) {
                                // show the coupon 
                                // add to allowed cpns
                                found = true
                                document.querySelector('.ct_search_cpns_outer .err_code').style.display = 'none'
                                addCpns(code.toLowerCase())

                            }
                        }
                    })
                    if (!found) {
                        document.querySelector('.ct_search_cpns_outer .success_code').style.display = 'none'

                        document.querySelector('.ct_search_cpns_outer .err_code').style.display = 'block'
                        document.querySelector('.ct_load_items').style.display = 'none'
                        addCpns('')
                    }
                }
                cpns()

            }

        })
        let btns = document.querySelectorAll('.ct_bx_buttons_actions')
        document.querySelector('html').addEventListener('click', (e) => {
            let bx2 = e.target.closest('.ct_slide_menu_acc')

            let slide = e.target.closest('.ct_field_small_size')
            if (slide !== null) {
                if (slide !== null) {
                    if (!document.querySelector('.ct_bx_slide_bx').classList.value.split(' ').includes('active'))
                        document.querySelector('.ct_bx_slide_bx').classList.add('active')
                    document.querySelector('.ct_bx_ov_lay').style.display = 'block'
                } else {
                    document.querySelector('.ct_bx_ov_lay').style.display = 'none'
                    document.querySelector('.ct_bx_slide_bx').classList.remove('active')

                }
            } else {
                if (e.target.closest('.essentials') !== null) {
                    return
                }
                if (document.querySelector('.ct_bx_slide_bx.active') !== null) {
                    document.querySelector('.ct_bx_slide_bx.active').classList.remove('active')
                    document.querySelector('.ct_bx_ov_lay').style.display = 'none'

                }

            }
        })


        btns.forEach(cur => {
            cur.addEventListener('click', (e) => {
                if (e.target.id === 'prev_crousel') {
                    iG--
                    document.querySelector('#forw').style['pointer-events'] = 'auto'
                    document.querySelector('#forw').style.opacity = '1'

                    if (iG !== 0) {
                        let calcT = iG * document.querySelector('.inner_slide').getBoundingClientRect().width
                        if (window.screen.width >= 1380) {
                            calcT = iG * 966
                        }
                        document.querySelectorAll('.inner_slide .fd_ek_cell_car_recom').forEach(row1 => {
                            row1.style.left = `-${calcT}px`
                            if (iG < 0) { iG = 0 }


                        })
                    } else {
                        document.querySelectorAll('.inner_slide .fd_ek_cell_car_recom').forEach(row1 => {

                            row1.style.left = `-0vw`
                        })
                        document.querySelector('#prev_crousel').style['pointer-events'] = 'none'
                        document.querySelector('#prev_crousel').style.opacity = '.5'
                        document.querySelector('#forw').style['pointer-events'] = 'auto'
                        document.querySelector('#forw').style.opacity = '1'
                    }
                } else {
                    iG++
                    document.querySelector('#prev_crousel').style['pointer-events'] = 'auto'
                    document.querySelector('#prev_crousel').style.opacity = '1'
                    let togo = Math.round(document.querySelectorAll('.fd_ek_cell_car_recom').length / 5) - 1
                    if (document.querySelector('#forw') !== null) {
                        document.querySelector('#forw').style['pointer-events'] = 'auto'
                        document.querySelector('#forw').style.opacity = '1'
                    }
                    if (iG === togo || togo === 0) {
                        document.querySelector('#forw').style['pointer-events'] = 'none'
                        document.querySelector('#forw').style.opacity = '.5'

                    }
                    if (togo === 0) { togo = 1 }
                    if (iG <= togo) {
                        let calcT = iG * document.querySelector('.inner_slide').getBoundingClientRect().width
                        if (window.screen.width >= 1380) {
                            calcT = iG * 966
                        }
                        document.querySelectorAll('.inner_slide .fd_ek_cell_car_recom').forEach(row1 => {
                            row1.style.left = `-${calcT}px`

                            if (iG === 0) { row1.style.left = `-0vw` }



                        })
                    }
                }

            })
        })
