        .ct_visit {
            background-color: blue;
            border-radius: 20px;
            padding: 10px 24px;
            color: white;
            width: max-content;
            margin: 40px 0px;
        }

        .bounce-in-left {
            animation: bounce-in-left 1.8s ease;
        }


        @keyframes bounce-in-left {
            0% {
                opacity: .6;
                transform: translateX(-400px);
            }

            60% {
                opacity: 1;
                transform: translateX(0px);
            }
        }

        .bounce-in-right {
            animation: bounce-in-right 2.3s ease;
        }

        @keyframes bounce-in-right {
            0% {
                opacity: .6;
                transform: translateX(2000px);
            }

            60% {
                opacity: 1;
                transform: translateX(0px);
            }
        }

        .gelatine {
            animation: gelatine 0.5s linear;
        }

        @keyframes gelatine {

            from,
            to {
                transform: scale(1, 1);
            }

            25% {
                transform: scale(0.9, 1.1);
            }

            50% {
                transform: scale(1.1, 0.9);
            }

            75% {
                transform: scale(0.95, 1.05);
            }
        }

        .bounce2 {
            animation: bounce2 2s ease;
        }

        @keyframes bounce2 {

            0%,
            20%,
            50%,
            80%,
            100% {
                transform: translateY(0);
            }

            40% {
                transform: translateY(-30px);
            }

            60% {
                transform: translateY(-15px);
            }
        }

        .prod_img {
            position: absolute;
        }
