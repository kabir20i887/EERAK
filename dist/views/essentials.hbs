            function getCookie(e) {
            for (var n = e + "=", t = decodeURIComponent(document.cookie)
                .split(";"), o = 0; o < t.length; o++) {
                for (var c = t[o];
                    " " == c.charAt(0);) c = c.substring(1);
                if (0 == c.indexOf(n)) return c.substring(n.length, c.length);
            }
            return "";
        }
    function load_basic_dt() {
            let total_itms = 0
            let allfd_sauce = JSON.parse(localStorage.getItem('sauce'))
            let allfd_essen = JSON.parse(localStorage.getItem('essentials'))
            if (allfd_essen !== null) {
                total_itms += allfd_essen.length
            }
            if (allfd_sauce !== null) {
                total_itms += allfd_sauce.length
            }
            if (total_itms > 9) {
                total_itms = '+9'
            }
            if (allfd_essen !== 0) {
                document.querySelector('.ct_info_cart').style.display = 'block'
                document.querySelector('.ct_info_cart p').innerText = total_itms
